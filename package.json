{
  "name": "easynotes",
  "displayName": "easynotes",
  "publisher": "moqsien",
  "description": "store notes easily",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/moqsien/easynotes.git"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:easynotes.click",
    "onView:view.easynotes"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [{
        "id": "easynotesView",
        "title": "easynotes",
        "icon": "./out/images/notes.svg"
      }]
    },
    "views": {
      "easynotesView": [{
        "name": "easynotes",
        "id": "view.easynotes"
      }]
    },
    "commands": [
      {
        "command": "easynotes.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/rimraf": "^3.0.2",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "stream": "^0.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "rimraf": "^3.0.2",
    "webdav": "^4.10.0"
  }
}
